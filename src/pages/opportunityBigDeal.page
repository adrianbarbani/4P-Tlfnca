    <script>
        $j = jq213; //jQuery.noConflict();
    
    $j(document).ready(function() {
        initializeToggles();
        
        $j(".oneshot").change(function(){
            console.log($j(".oneshot").val());
            console.log({!Survey.Full_contract_value__c});
            
            if($j(".oneshot").val() != null && {!Survey.Full_contract_value__c} != null){
                
                console.log('entre al if');
                var resta = {!Survey.Full_contract_value__c} - $j(".oneshot").val();
                console.log(resta);
                $j(".ingresosRecurrentes").val(resta);
            }
            //ingresosRecurrentes = fcv - oneshot
        });
        
        //Sliders ================================================
        
        var handle = $( "#custom-handle" );
        $( "#lineasImplicadas" ).slider({
            range: "max",
            min: 0,
            max: 100,
            value: {!Survey.Lineas_implicadas__c},
            step: 10,
            create: function() {
                handle.text( $( this ).slider( "value" ) );
            },
            slide: function( event, ui ) {
                $( ".lineasImplicadas" ).val( ui.value );
                handle.text( ui.value+'%' );
            }
        });
        
        $( ".lineasImplicadas" ).val( $( "#lineasImplicadas" ).slider( "value" ) );
        //========================================================================================
        
        $j("#" + GetURLParameter("section")).click();
        captureFileButtons();
    });
    

    
    function captureFileButtons() {
        <apex:outputText rendered="{!canEdit}">
            $j("[name=newNote]").on("mousedown", function(){
            SaveChanges();
        });
        $j("[name=attachFile]").on("mousedown", function(){
            SaveChanges();
        });            
        $j("[name=viewAll]").on("mousedown", function(){
            SaveChanges();
        });
        </apex:outputText>
    }
    
    function GetURLParameter(sParam) {
        
        var sPageURL = window.location.search.substring(1);
        var sURLVariables = sPageURL.split('&');
        
        for (var i = 0; i < sURLVariables.length; i++) {
            var sParameterName = sURLVariables[i].split('=');
            if (sParameterName[0] == sParam) {
                return sParameterName[1];
            }
        }
    }
    
    function ajaxshow () {
        $j(".ajaxloader").show();
    }
    function ajaxstop () {
        $j(".ajaxloader").hide();
    }
    
    function initializeToggles () {
        var toggle = $j("[id$=reach-defined-checkbox]");
        /*
        toggle.click(function () {
            toggleRowClass("reach-file-upload", this.checked);
            toggleRow("reach-file-valoration", this.checked);
        });
        toggleRowClass("reach-file-upload", toggle.is(':checked'));
        toggleRow("reach-file-valoration", toggle.is(':checked'));
        */
        toggle.click(function () {
            toggleRowClass("reach-file-upload", $j(this).find('option:selected').val() === "Si");
            toggleRow("reach-file-valoration", $j(this).find('option:selected').val() === "Si");
        });
        toggleRowClass("reach-file-upload", toggle.find('option:selected').val() === "Si");
        toggleRow("reach-file-valoration", toggle.find('option:selected').val() === "Si");
        //---------------------------------------------------------------------
        toggle = $j("[id$=reach-similar-projects-exp]");
        toggle.click(function () {
            toggleRow("reach-similar-projects-exp-detail", 
                      $j(this).find('option:selected').val() === "Si");
        });
        toggleRow("reach-similar-projects-exp-detail", toggle.find('option:selected').val() === "Si");
        
        //---------------------------------------------------------------------
        toggle = $j("[id$=exigencia_de_slas]");
        toggle.click(function () {
            toggleRowClass("exigencia_de_slas_desplegable",$j(this).find('option:selected').val() === "Si");
            toggleRowClass("hay_slas_desplegable", ($j(this).find('option:selected').val() === "Si" && $j("[id$=hay_slas]").find('option:selected').val() === "Si"));
        });
        toggleRowClass("exigencia_de_slas_desplegable", toggle.find('option:selected').val() === "Si");
        toggleRowClass("hay_slas_desplegable", (toggle.find('option:selected').val() === "Si" && $j("[id$=hay_slas]").find('option:selected').val() === "Si"));
        //---------------------------------------------------------------------
        toggle = $j("[id$=hay_slas]");
        toggle.click(function () {
            toggleRowClass("hay_slas_desplegable", 
                           ($j(this).find('option:selected').val() === "Si" && $j("[id$=exigencia_de_slas]").find('option:selected').val() === "Si"));
        });
        toggleRowClass("hay_slas_desplegable", (toggle.find('option:selected').val() === "Si" && $j("[id$=exigencia_de_slas]").find('option:selected').val() === "Si"));
        //---------------------------------------------------------------------
        toggle = $j("[id$=esClienteActual]");
        toggle.click(function () {
            toggleRowClass("esClienteActualAMostrar", 
                           $j(this).find('option:selected').val() === "Si");
        });
        toggleRowClass("esClienteActualAMostrar", toggle.find('option:selected').val() === "Si");
        
        //---------------------------------------------------------------------
        toggle = $j("[id$=reach-technical-checkbox]");
        /*
        toggle.click(function () {
            toggleRowClass("reach-technical-text", this.checked);
            toggleRow("reach-technical-valoration", this.checked);
        });
        toggleRowClass("reach-technical-text", toggle.is(':checked'));
        toggleRow("reach-technical-valoration", toggle.is(':checked'));
        */
        toggle.click(function () {
            toggleRowClass("reach-technical-text", $j(this).find('option:selected').val() === "Si");
            toggleRow("reach-technical-valoration", $j(this).find('option:selected').val() === "Si");
        });
        toggleRowClass("reach-technical-text", toggle.find('option:selected').val() === "Si");
        toggleRow("reach-technical-valoration", toggle.find('option:selected').val() === "Si");
        //---------------------------------------------------------------------
        toggle = $j("[id$=reach-third-party-required-checkbox]");
        toggle.click(function () {
            toggleRow("reach-third-party-amount", $j(this).find('option:selected').val() === "Si");
        });
        toggleRow("reach-third-party-amount", toggle.find('option:selected').val() === "Si");
        //---------------------------------------------------------------------
        toggle = $j("[id$=reach-hire-checkbox]");
        toggle.click(function () {
            toggleRow("reach-hire-amount", $j(this).find('option:selected').val() === "Si");
        });
        toggleRow("reach-hire-amount", toggle.find('option:selected').val() === "Si");
        //---------------------------------------------------------------------
        toggle = $j("[id$=team-fte-not-present]");
        var toggleB = $j("[id$=team-fte-third-party]");
        var toggleFunction = function () {
            toggleRow("team-fte-detail", 
                      ($j("[id$=team-fte-not-present]").find('option:selected').val() !== "" && 
                       $j("[id$=team-fte-not-present]").find('option:selected').val() !== "0") ||
                      ($j("[id$=team-fte-third-party]").find('option:selected').val() !== "" && 
                       $j("[id$=team-fte-third-party]").find('option:selected').val() !== "0"));
        }
        toggle.click(toggleFunction);
        toggleB.click(toggleFunction);
        toggleFunction();
        //---------------------------------------------------------------------
        toggle = $j("[id$=risk-project-SLA-checkbox]");
        toggle.click(function () {
            toggleRowClass("risk-project-SLA-detail", this.checked);
            toggleRow("risk-project-SLA-valoration", this.checked);
        });
        toggleRowClass("risk-project-SLA-detail", toggle.is(':checked'));
        toggleRow("risk-project-SLA-valoration", toggle.is(':checked'));
    }
    
    function toggleRow(id, condition) {
        //the closest("tr") obtain the element that is generated as 
        //part of the pageBlockSection that contains both the title and
        //the input field
        if(condition) {
            $j("[id$=" + id + "]").closest("tr").show("fast");
        }
        else {
            $j("[id$=" + id + "]").closest("tr").hide("fast");
        }
    }
    
    function toggleRowClass(aClass, condition) {
        //the closest("tr") obtain the element that is generated as 
        //part of the pageBlockSection that contains both the title and
        //the input field
        if(condition) {
            $j("." + aClass).closest("tr").show("fast");
        }
        else {
            $j("." + aClass).closest("tr").hide("fast");
        }
    }
    </script>
    
