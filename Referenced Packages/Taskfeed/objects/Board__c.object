    <validationRules>
        <fullName>MAX_LIST_COUNT</fullName>
        <active>true</active>
        <description>Restrict number of lists on a board to 12</description>
        <errorConditionFormula>(Number_of_Lists__c &gt; PRIORVALUE( Number_of_Lists__c )) &amp;&amp; Number_of_Lists__c &gt;= 12</errorConditionFormula>
        <errorMessage>To protect performance a board can have a maximum of 12 lists</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MAX_TASK_COUNT</fullName>
        <active>true</active>
        <description>Restrict number of tasks on a board to 500</description>
        <errorConditionFormula>/*** if the number of tasks has increased  and over 500 **/

(Number_of_Tasks__c &gt; PRIORVALUE( Number_of_Tasks__c )) &amp;&amp; Number_of_Tasks__c &gt; 500</errorConditionFormula>
        <errorMessage>To protect performance a board can have a maximum of 500 tasks</errorMessage>
    </validationRules>
